{
    "compilerOptions": {
        "alwaysStrict": true,
        "noImplicitAny": true,
        "declaration": true,
        "strict": true,
        "target": "ES6",
        "moduleResolution": "Node",
        "module": "CommonJS",
        "allowJs": true,
        "rootDirs": [ "src/", "test/"],
        "outDir": "dist/",
        "esModuleInterop": true,
        "incremental": true,
        "tsBuildInfoFile": "dist/.tsbuildinfo",
        "strictNullChecks": true
        
    }
}